<h1>salut</h1>
<%  id = @music.year_id%>
<% @category.musics.each do |music|%>
  <%if music.year_id == id%>
    <p><%= music.title%></p>
    <iframe
      width="560" height="315" src="https://www.youtube.com/embed/<%=music.source%>" title="YouTube video player"
      frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
      allowfullscreen>
    </iframe>
  <%end%>
<%end%>
